  "scripts": {
    "prepare": "husky install",
    "start:sh": "git pull origin main && npm run db:push && sh scripts/start.sh",
    "lint:affected": "nx affected -t lint",
    "all": "docker compose up -d && nx run-many --target=serve --host=0.0.0.0",
    "build:all": "nx run-many --target=build --configuration=production",
    "migration:generate": "drizzle-kit generate:pg --config=./libs/mx-schema/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=./libs/mx-schema/drizzle.config.ts",
    "db:drop:migration": "drizzle-kit drop --config=./libs/mx-schema/drizzle.config.ts",
    "drizzle-kit:studio": "drizzle-kit studio --config=./libs/mx-schema/drizzle.config.ts --port 4001",
    "generate:code": "plop --plopfile _plop/generators/index.js"
  },
  "private": true,
