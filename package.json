    "migration:g": "drizzle-kit generate --config=./libs/mx-schema/drizzle.config.ts",
    "migrate": "drizzle-kit migrate --config=./libs/mx-schema/drizzle.config.ts",
    "db:up": "drizzle-kit up --config=./libs/mx-schema/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=./libs/mx-schema/drizzle.config.ts",
    "db:drop:migration": "drizzle-kit drop --config=./libs/mx-schema/drizzle.config.ts",
    "drizzle-kit:studio": "drizzle-kit studio --config=./libs/mx-schema/drizzle.config.ts --port 4001",
    "generate:code": "plop --plopfile _plop/generators/index.js",
    "dc:dev": "docker compose  -f docker-compose.dev.yml up -d",
    "dc:prod": "docker compose  -f docker-compose.prod.yml up -d",
    "db:seed": "tsx ./apps/api/src/db/seed/seed.postgres.ts"
  },
